{
	"manifest_version": 2,

	"name": "Spartan Security",
	"description": "Security monitering extension",
	"version":"1.0.0",
	"icons": {
		"16": "fav1.png"
	},

	"background": {
		"persistent": false,
		"scripts": ["background.js"]
	},

	"content_scripts": [{
		"matches": [
			"<all_urls>"
		],
		"js":["content.js"]
	}],

	"browser_action": {
		"default_popup": "popup.html"
	},

	"permissions": [
		"storage"
	]
}
